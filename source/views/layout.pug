block data
    -var template = "default"
doctype
html
    head
        block meta
            //- Required meta tags
            meta(charset='utf-8')
            meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')

        block css
            link(href='https://fonts.googleapis.com/css?family=Arvo', rel='stylesheet', type='text/css')
            link(rel='stylesheet', href='assets/css/bnbs.min.css')
            //-link(rel='stylesheet', href='css/app.css')


        block head-scripts
            script(type='text/javascript', src='//widget.trustpilot.com/bootstrap/v5/tp.widget.bootstrap.min.js', async='')


        block links
            //- link(rel='profile', href='')
            //-link(rel='shortcut icon', href='http://www.monacoriches.com/favicon.png', type='image/x-icon')
            //-link(rel='canonical', href='http://www.monacoriches.com/')

    //block vars
    body
        block header
            include includes/svg/svg
            include includes/header/header


        .wrapper

            block content



        block footer
            include includes/footer/footer




        block scripts
            script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js')
            
            script.
                $(function () {
                    $(window).bind("resize", function () {
                        console.log($(this).width())
                        if ($(this).width() < 768) {
                            $('.bingo-site').removeClass('sticky-top')
                        } else {
                            $('.bingo-site').addClass('sticky-top')
                        }
                    }).trigger('resize');
                })

            //-script(src='/assets/js/tether.min.js')
            //-script(src='/assets/js/bootstrap.min.js')
            //-script(src='assets/js/jquery.bxslider.min.js')
            //-script(src='assets/js/bxSliderHome.js')
            script(src="assets/js/bnbs.min.js")
